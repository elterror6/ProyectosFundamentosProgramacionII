package Catalogo;

public class Catalogo<E> {
	private E catalog[];
	
	public Catalogo(E[] catalog) {
		this.catalog = catalog; 
		
	}
	
	public E[] getCatalog() {
		return this.catalog;
	}
	
	public void setCatalog(E[] catalog) {
		this.catalog = catalog;
	}
	
	public void setE(E e, int n) {
		this.catalog[n] = e; 
	}
}
